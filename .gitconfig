[user]
	email = heilalmond@grinnell.edu
	name = Almond Heil
[color]
	ui = auto
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	editor = nvim
	excludesFile = ~/.gitignore
[alias]
	tree = log --graph --oneline
	slog = log --oneline
[pull]
	rebase = false
